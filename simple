###  https://raw.githubusercontent.com/eric-jahn/niemtest/main/simple/test#isPrivate
niem:isPrivate rdf:type owl:ObjectProperty ;
               rdfs:domain owl:Thing ;
               rdfs:range [ rdf:type owl:Restriction ;
                            owl:onProperty niem:private ;
                            owl:someValuesFrom xsd:boolean
                          ] .


###  https://raw.githubusercontent.com/eric-jahn/niemtest/main/simple/test#ownedItem
niem:ownedItem rdf:type owl:ObjectProperty ;
               rdfs:domain niem:Ownership ;
               rdfs:range niem:Person .


###  https://raw.githubusercontent.com/eric-jahn/niemtest/main/simple/test#owner
niem:owner rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain niem:Ownership ;
           rdfs:range niem:Item .


###  https://raw.githubusercontent.com/eric-jahn/niemtest/main/simple/test#ownsItem
niem:ownsItem rdf:type owl:ObjectProperty ;
              rdfs:domain niem:Person ;
              rdfs:range niem:Item .


